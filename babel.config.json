{
  "env": {
    "esmUnbundled": {
      "ignore": ["tests/**/*.ts", "src/types.ts"],
      "presets": ["@babel/preset-typescript"]
    },
    "esmBundled":{
      "ignore": ["tests/**/*.ts", "src/types.ts"],
      "presets": [
        "@babel/preset-typescript",
        ["@babel/preset-env", {
          "targets": {
            "node": "20"
          }
        }]
      ]
    },
    "cjs": {
      "ignore": ["tests/**/*.ts", "src/types.ts"],
      "presets": [
        "@babel/preset-typescript",
        ["@babel/preset-env", {
          "targets": {
            "node": "20"
          },
          "modules": "commonjs"
          }]
      ],
      "plugins": ["add-module-exports"]
    }
  }
}
