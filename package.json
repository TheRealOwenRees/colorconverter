{
  "name": "colorconvertor",
  "version": "0.0.1",
  "description": "",
  "main": "lib/cjs/colorconvertor.js",
  "type": "module",
  "module": "lib/esm/colorconvertor.js",
  "exports": {
    "require": "./lib/cjs/colorconvertor.js",
    "import": "./lib/esm/colorconvertor.js"
  },
  "types": "/lib/types/index.d.ts",
  "scripts": {
    "build": "npm run removeBuild && npm run build:esm && npm run build:cjs && tsc --project tsconfig-build.json && npm run rollup",
    "removeBuild": "rm -rf lib/esm && rm -rf lib/cjs && rm -rf lib/types",
    "build:esm": "BABEL_ENV=esmUnbundled babel src --extensions \".ts\" --out-dir 'lib/esm' --source-maps",
    "build:cjs": "BABEL_ENV=cjs babel src --extensions \".ts\" --out-dir 'lib/cjs' --source-maps",
    "rollup": "rollup -c",
    "test": "jest"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@babel/cli": "^7.23.4",
    "@babel/preset-env": "^7.23.8",
    "@babel/preset-typescript": "^7.23.3",
    "@rollup/plugin-babel": "^6.0.4",
    "@rollup/plugin-node-resolve": "^15.2.3",
    "@rollup/plugin-terser": "^0.4.4",
    "@types/jest": "^29.5.11",
    "@types/node": "^20.10.7",
    "@typescript-eslint/eslint-plugin": "^6.4.0",
    "@typescript-eslint/parser": ">=6.0.0",
    "eslint": "^8.0.1",
    "eslint-config-standard-with-typescript": "^43.0.0",
    "eslint-plugin-import": "^2.25.2",
    "eslint-plugin-n": "^15.0.0",
    "eslint-plugin-promise": "^6.0.0",
    "jest": "^29.7.0",
    "prettier": "^3.1.1",
    "rollup": "^4.9.4",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.2",
    "tsdoc": "^0.0.4",
    "typescript": "*"
  }
}
